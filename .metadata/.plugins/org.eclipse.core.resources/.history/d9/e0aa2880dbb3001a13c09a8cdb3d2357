package com.syntax.class11;

public class Brask01 {
public static void main(String [] args) {
	String[] cars = {"Honda", "Jetta", "BMW", "Ford", "Benz", "Cadillac", "Range"};
	
	
	for (String elements:cars) {
	System.out.println(elements);
	}
	System.out.println("======================================");
	
	
	
	
	int[] numbers = {1, 2, 3, 4, 5};
	int sum = 0;
	for (int num:numbers) {
		sum += num;
	}
	
	System.out.println("The sum of all number is: " + sum);

	
			System.out.println("==================================");
}
}
