package com.syntax.repl;

import java.util.Scanner;

public class HW063 {
	  public static void main(String[] args) {
		    Scanner inp = new Scanner(System.in);
		    System.out.print("In:");
		    int x = inp.nextInt();
		    
		    for (int x = 1; x <= 10; x++) {
		    	System.out.println(x);
		    }
		    
inp.close();
		  }
		}
