package com.syntax.class08;

public class Task1 {

	public static void main(String[] args) {
		// Print numbers from 1 to 50 except those that are divisible by 3

		for (int i = 1; i<=50; i++){
			System.out.println(i);
			if ((i % 3) == 3) {
				continue;
			}
			System.out.println(i);
		}
		
	}

}
